<xsd:schema elementFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ups="http://www.ups.com/XMLSchema">  <xsd:element name="AddressValidationRequest">    <xsd:complexType>      <xsd:sequence>        <xsd:element name="Request" type="RequestType"/>        <xsd:element minOccurs="0" name="RegionalRequestIndicator"/>        <xsd:element minOccurs="0" name="MaximumListSize"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="AddressKeyFormat" type="AddressKeyFormatType"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="AddressKeyFormatType">    <xsd:sequence>      <xsd:element minOccurs="0" name="ConsigneeName" type="xsd:string"/>      <xsd:element minOccurs="0" name="BuildingName" type="xsd:string"/>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AddressLine" type="xsd:string"/>      <xsd:choice>        <xsd:element minOccurs="0" name="Region" type="xsd:string"/>        <xsd:sequence>          <xsd:element minOccurs="0" name="PoliticalDivision2" type="xsd:string"/>          <xsd:element minOccurs="0" name="PoliticalDivision1" type="xsd:string"/>          <xsd:element minOccurs="0" name="PostcodePrimaryLow" type="xsd:string"/>          <xsd:element minOccurs="0" name="PostcodeExtendedLow" type="xsd:string"/>        </xsd:sequence>      </xsd:choice>      <xsd:element minOccurs="0" name="Urbanization" type="xsd:string"/>      <xsd:element name="CountryCode" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="RequestType">    <xsd:sequence>      <xsd:sequence>        <xsd:element minOccurs="0" name="TransactionReference" type="TransactionReferenceType"/>        <xsd:element name="RequestAction" type="xsd:string"/>        <xsd:element minOccurs="0" name="RequestOption" type="xsd:string" ups:usage="notused"/>      </xsd:sequence>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="TransactionReferenceType">    <xsd:sequence>      <xsd:element minOccurs="0" name="CustomerContext" type="xsd:string"/>      <xsd:element minOccurs="0" name="ToolVersion" type="xsd:string" ups:usage="notused"/>    </xsd:sequence>  </xsd:complexType></xsd:schema>